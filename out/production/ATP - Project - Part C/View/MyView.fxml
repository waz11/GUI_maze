<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="433.0" prefWidth="636.0" stylesheets="@MainStyle.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MyViewController">
  <left>
    <VBox prefHeight="200.0" prefWidth="178.0" spacing="5.0" BorderPane.alignment="CENTER">
      <children>
        <GridPane>
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <Label text="Maze rows:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
          <Label text="Maze columns:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
          <TextField fx:id="rowsInput" text="50" GridPane.columnIndex="1" GridPane.rowIndex="0" />
          <TextField fx:id="colsInput" text="50" GridPane.columnIndex="1" GridPane.rowIndex="1" />
          <Button fx:id="#btn_generateMaze" onAction="#generateMaze" prefHeight="25.0" prefWidth="167.0" text="Generate Maze :)" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="2" />
          <Button fx:id="#btn_solveMaze" layoutX="10.0" layoutY="73.0" onAction="#solveMaze" prefHeight="25.0" prefWidth="178.0" text="Solve Maze :(" GridPane.columnSpan="2" GridPane.rowIndex="3" />
        </GridPane>
      </children>
      <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
    </VBox>
  </left>
  <bottom>
    <HBox prefHeight="26.0" prefWidth="636.0" BorderPane.alignment="CENTER">
      <children>
        <Label text="Status bar" />
      </children>
    </HBox>
  </bottom>
  <top>
    <VBox BorderPane.alignment="CENTER">
      <children>
        <MenuBar>
          <menus>
            <Menu mnemonicParsing="false" text="File">
              <items>
                <MenuItem mnemonicParsing="false" text="Close" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
              <items>
                <MenuItem mnemonicParsing="false" text="Delete" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
              <items>
                <MenuItem mnemonicParsing="false" text="About" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
      </children>
    </VBox>
  </top>
  <center>
    <Pane BorderPane.alignment="CENTER">
      <children>
<!--        <MazeDisplayer fx:id="mazeDisplayer" height="400.0" width="400.0" />-->
      </children>
      <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
    </Pane>
  </center>
</BorderPane>
